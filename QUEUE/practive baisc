// Online C++ compiler to run C++ program online
#include <iostream>
#include<queue>
using namespace std;

int main() {
  queue<int> q;
   q.push(11);
   q.push(10);
   q.push(111);
   q.push(101);
    cout<<"size of queue is :"<<q.size()<<endl;
    //q.front();
    q.pop();
  // q.pop();  q.pop();  q.pop();   
    cout<<"size of queue is :"<<q.size()<<endl;
    

    if(q.empty()){
        cout<<"queue is empty"<<endl;
    }
    else{
        cout<<"queeue is not empty"<<endl;
    }
    return 0;
} 
