#include<iostream>
using namespace std;

class stack{
    public:
    int *arr;
    int top;
    int size;

     stack (int size){
         this->size=size;
         arr=new int[size];
         top =-1;

     }
    void push (int element ){
        if(size-top>1){
            top++;
            arr[top]=element;
        }
        else{
            cout<<"stack OVerFlow"<<endl;
        }
    }
    void pop()
    {
        if(top>=0){
            top--;

        }
        else {
            cout<<"stack Underflow"<<endl;

        }

    }
    int peek(){
        if (top>=0 && top<size)
        {
        return arr[top];
        }
         else {
            cout << "Stack is Empty" << endl;
            return -1;}

    }

  //* void isEmpty(){
    //    if (top ==-1){
    //        return 1;
    //    }
    //    else {return -1;}
    //}
};
int main(){
    stack st(5);
    st.push(10);
    st.push(15);
    st.push(20);
    st.push(25);
    st.push(30);
    st.pop();
    st.pop();
    st.pop();
    st.pop();
    st.pop();
    cout<<st.peek()<<endl;

}
