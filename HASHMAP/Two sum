#include <iostream>
#include<vector>
using namespace std;
bool twosum(int arr[],int n,int target){
    //int n=arr.size();
    for(int i=0;i<n;i++){
        for (int j=0;j<i+1;j++){
            if (arr[i]+arr[j]==target){
                return true;
            }
        }
    }
    return false;
}
int main() {

int arr[]={1,2,3,4,5};
int target=2;
int n=5;
if(twosum(arr,n,target)){
cout<<"true";
}
else{
cout<<"false";}
    return 0;
}
