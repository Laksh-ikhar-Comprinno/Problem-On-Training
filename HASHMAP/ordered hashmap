#include <iostream>
#include<map>
#include<ordered_map>
using namespace std;
int main() {
    unordered_map<string,int> m;
    
    pair<string,int> pair1("laksh",1);
    m.insert(pair1);
    pair<string,int> pair2("laksh1",2);
    m.insert(pair2);
    m["shree"]=4;
    m["shri"]=2;
    cout<<m["laksh2"]<<endl;
    cout<<m.at("laksh3")<<endl;
    cout<<m.size()<<endl;
    cout<<m.count("laksh")<<endl;
    cout<<m.count("hai hi nahi")<<endl;
    m.erase("shri");
    cout<<m.size()<<endl;
    for(auto i:m){
        cout<<i.first<<" "<< i.second <<endl;
    }
    ordered_map<string,int>::iterator it=m.begin();
    
   //iterator way of giving hash map with it map functiond
   
    while(it !=m.end()){
        cout<<it->first<<" "<< it->second<<endl;
        it++;
        
    }
    return 0;
}
